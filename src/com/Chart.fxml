<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="All" minHeight="500" minWidth="500" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ChartController">
			<HBox alignment="CENTER">
      		<HBox fx:id="chartPage" alignment="CENTER">
      			<Button fx:id="Top" mnemonicParsing="false" onAction="#goTop" text="Top" />
      			<Button fx:id="Down" mnemonicParsing="false" onAction="#goDown" text="Down" />
			</HBox>
         	<VBox prefHeight="100.0" prefWidth="100.0" id="dx">
               	<Label text="D/d" />
      				<Label id="dd" fx:id="dd" textAlignment="CENTER" />
        	 </VBox>
         	<VBox alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="100.0" id="rx">
              	 <Label text="Ri/Rm" />
               		<Label id="rirm" fx:id="rirm" />
        	 </VBox>
			</HBox>

			<VBox fx:id="chartBox" prefHeight="800" prefWidth="500" HBox.hgrow="ALWAYS">

			</VBox>
   <padding>
      <Insets top="5.0" />
   </padding>
</VBox>
